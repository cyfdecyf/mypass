{
  "manifest_version": 2,
  "name": "MyPass",
  "description": "Keep password by yourself",
  "version": "0.1",
  "icons": {
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_popup": "popup.html?init=1"
  },
  "options_page": "options.html",
  "permissions": [ "tabs", "storage" ]
}
