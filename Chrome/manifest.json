{
  "manifest_version": 2,
  "name": "MyPass",
  "description": "Generate different passwords for different sites with a single passphrase.",
  "version": "1.4",
  "icons": {
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png",
      "38": "icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [ "tabs", "storage" ]
}
